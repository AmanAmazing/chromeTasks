{
  "manifest_version": 3,
  "name": "Task Tracker",
  "version": "1.0",
  "description": "A task tracking Chrome extension",
  "icons":{
    "16":"icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Alt+T"
      }
    }
  }
}
